{
	"version": "2.25.1",
	"name": "burger-editor",
	"description": "CSS, JavaScript & Images for BurgerEditor",
	"private": true,
	"main": "none",
	"scripts": {
		"build": "webpack --mode=production --config ./client/webpack.config.ts; gulp css-editor;",
		"build:addon": "tsc -p ./Addon/tsconfig.json",
		"build:css": "gulp css-public",
		"dev": "webpack --mode=development --watch --config ./client/webpack.config.ts",
		"dev:addon": "tsc -w -p ./Addon/tsconfig.json",
		"test:lite": "ava --color Addon/type/**/test.js src/**/*.test.js",
		"test": "nyc ava --color Addon/type/**/test.js src/**/*.test.js && nyc report --reporter=html",
		"lint": "prettier --write './client/**/*.{js,ts}' './Addon/**/*.ts'; eslint --fix './client/**/*.{js,ts}' './Addon/**/*.ts'",
		"watch:test": "chokidar \"src/**/*.ts\" \"Addon/**/*.ts\" -c \"npm test\"",
		"icons:copy": "find Addon/svg -name '*.svg' | sed -e '/copy/! s/\\(Addon\\/svg\\/\\([a-z0-9-]*\\)\\.svg\\)/cp \\1 Addon\\/block\\/\\2\\/panel.svg/g' | sh",
		"production": "gulp production"
	},
	"ava": {
		"require": [
			"./src/test/helper/setup-browser-env.js"
		]
	},
	"repository": {
		"type": "git",
		"url": "git@lab.e-catchup.jp:catchup/burger_editor.git"
	},
	"author": "D-ZERO Co.,Ltd.",
	"license": "BurgerEditor License",
	"browserslist": [
		"Explorer >= 11",
		"last 2 version"
	],
	"devDependencies": {
		"@types/ckeditor": "0",
		"@types/google-maps": "3.2.2",
		"@types/jquery": "3.5.0",
		"@types/jquery.colorbox": "1.4.30",
		"@types/jqueryui": "1.12.13",
		"@types/js-base64": "2.3.2",
		"@types/node": "14.0.19",
		"@types/object-fit-images": "3.2.0",
		"@types/semver": "6",
		"@types/webpack": "4.41.21",
		"@types/webpack-dev-server": "3.11.0",
		"@types/youtube": "0.0.39",
		"@typescript-eslint/eslint-plugin": "3.6.0",
		"@typescript-eslint/parser": "3.6.0",
		"autoprefixer": "9.8.4",
		"ava": "0",
		"browser-env": "3.3.0",
		"chokidar-cli": "2.1.0",
		"cssnano": "4.1.10",
		"eslint": "7.4.0",
		"eslint-plugin-eslint-comments": "3.2.0",
		"eslint-plugin-jsdoc": "28.6.1",
		"glob": "7.1.6",
		"gulp": "4.0.2",
		"gulp-iconfont": "9",
		"gulp-postcss": "8.0.0",
		"gulp-sass": "5.1.0",
		"gulp-zip": "4",
		"nyc": "11",
		"postcss-base64": "0.7.1",
		"postcss-calc": "7.0.2",
		"postcss-clip-path-polyfill": "1.0.4",
		"postcss-color-function": "4.1.0",
		"postcss-custom-media": "7.0.8",
		"postcss-gap-properties": "2.0.0",
		"postcss-import": "12.0.1",
		"postcss-math": "0.0.10",
		"postcss-object-fit-images": "1.1.2",
		"prettier": "2.0.5",
		"sass": "1.32",
		"stylelint": "13.6.1",
		"stylelint-config-standard": "20.0.0",
		"stylelint-order": "4.1.0",
		"stylelint-scss": "3.18.0",
		"ts-loader": "7.0.5",
		"ts-node": "8.10.2",
		"typescript": "3.9.6",
		"webpack": "4.43.0",
		"webpack-cli": "3.3.12"
	},
	"dependencies": {
		"@burger-editor/frozen-patty": "0.10.0",
		"baserjs": "1.0.0-beta.0",
		"core-js": "3.6.5",
		"jaco": "4.0.0",
		"jquery": "3.5.1",
		"js-base64": "2.6.2",
		"object-fit-images": "3.2.4",
		"psycle": "1.0.0-beta.4",
		"semver": "6"
	}
}
